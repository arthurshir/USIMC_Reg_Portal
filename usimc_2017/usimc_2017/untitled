
// x: 10,.300
// Colors: Client, P2P

F = 1500
u_s = 30
d_min = 2
N_arr = [10, 100, 1000]
u_arr = [.300, .700, 2]

def calculate(N, F):
	d_cs = max( (N*F)/u_s, F/d_min )
	sum = u_s
	for i in range(1, N + 1):
		sum += u
	d_p2p = max( F/u_s, F/d_min, (NF)/sum )
	print("N: " + N + " F: " + F + "\n" + "Client Server = " + d_cs + "\nP2P: " + d_p2p + "\n")

for i in range(length(N_arr)):
	for j in range(length(u_arr)):
		calculate(N_arr(i), u_arr(j)


